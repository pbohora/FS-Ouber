		"gql-merge": "^0.0.6",
		"graphql-to-typescript": "^0.1.2",
		"prettier": "^2.2.0",
		"ts-node": "^9.0.0",
		"ts-node-dev": "^1.0.0",
		"tslint-config-prettier": "^1.18.0",
